package com.zim.member.searchid;

import com.zim.member.MemberDao;
import com.zim.member.MemberVO;

/**
 * @author sist
 *
 */
public class SearchIdService {
	MemberDao memberDao = new MemberDao();
	
	public String do_searchId(String inputEmail){
		String outStr = "";
		//db이메일==이메일 체크
		MemberVO tmpVO = new MemberVO();
		tmpVO.setEmail(inputEmail);
		tmpVO = (MemberVO) memberDao.do_selectOne_ckId(tmpVO);
		outStr = tmpVO.getUserId();
		return outStr;
	}

}
