	pageEncoding="UTF-8"%>
<%
	ProductVO vo = new ProductVO();
	ImageVO imgVO = new ImageVO();
%>
<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
